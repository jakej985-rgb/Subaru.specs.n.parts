<?xml version="1.0" encoding="utf-8" ?>
<base:ThemedContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:base="clr-namespace:SubaruParts.App.Pages.Base"
             x:Class="SubaruParts.App.Pages.Specs.TorquePage"
             Title="Torque">
    <VerticalStackLayout Spacing="10" VerticalOptions="Center" HorizontalOptions="Center">
        <Label Text="Torque"
               FontSize="32"
               FontAttributes="Bold"
               TextColor="{StaticResource OemText}"
               HorizontalOptions="Center" />
        <Label Text="Coming soon."
               FontSize="18"
               TextColor="{StaticResource OemMuted}"
               HorizontalOptions="Center" />
    </VerticalStackLayout>
</base:ThemedContentPage>
